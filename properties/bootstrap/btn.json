{
  "btn": {
    "font": {
      "family": { "value": "{font.family.base}" },
      "size": { "value": "{font.size.base.value}" },
      "weight" :{ "value" :"{font.weight.normal.value}"}
    },
    "border": {
      "radius": { "value": "{border.radius.value}" },
      "width" :{ "value" : "{input.btn.broder.width}"}
    },
    "padding":{
      "y":{ "value":"{input.btn.padding.y.value}"},
      "x":{ "value":"{input.btn.padding.x.value}"}
    },
    "link":{
      "color":{"value":"{input.btn.link.color.value}"},
      "hover":{
        "color":{"value":"{input.btn.broder.width}"}
      }
    }
  }
}
